{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": "",
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "jquery",
      "materialize-css"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "include": [
    "src/**/*",
    "src/**/*.ts"
  ]
}
